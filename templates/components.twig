                    {% for opt in range(1,5) %}
                    <div class="filter-dropdown-menu-item">
                        <input type="checkbox" id="option_{{opt}}" name="option_{{opt}}" />
                        <label for="option_{{opt}}">Option {{opt}}</label>
                    </div>
                    {% endfor %}
                </div>
            </div>

        </div>


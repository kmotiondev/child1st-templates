            </div>

        </div>

        {# Buttons #}
        <h1 class="heading-3xl">Buttons</h1>
        <div class="border-y border-blue-100 py-4 lg:py-8">

            <div class="flex gap-8 items-stretch flex-col lg:flex-row lg:items-center">
                <div>
                    <button class="button">Primary</button>
                </div>
                <div>
                    <button class="button button--secondary">Secondary</button>
                </div>
                <div>
                    <button class="button button--tertiary">Tertiary</button>
                </div>
            </div>

        </div>

        {# Inputs #}
        <h1 class="heading-3xl">Inputs</h1>
        <div class="border-y border-blue-100 py-4 lg:py-8">

            <div>
                <input type="text" class="text-input lg:w-96 mb-6" name="input-example-01" id="input-example-01" placeholder="Input text">
            </div>
            <div>
                <select class="select-input lg:w-96 mb-6" name="select-example-02" id="select-example-02" placeholder="Input Text">
                    <option value="01" selected>Input Text</option>
                    <option value="02">Input Text</option>
                    <option value="03">Input Text</option>
                </select>
            </div>

        </div>

        {# Inputs With Labels #}
        <h1 class="heading-3xl">Inputs With Labels</h1>
        <div class="border-y border-blue-100 py-4 lg:py-8">

            <div>
                <label class="input-label" for="text-example-02">Label</label>
                <input type="text" class="text-input lg:w-96 mb-6" name="text-example-02" id="text-example-02" placeholder="Input Text">
            </div>
            <div>
                <label class="input-label" for="select-example-02">Label</label>
                <select class="select-input lg:w-96 mb-6" name="select-example-02" id="select-example-02" placeholder="Input Text">
                    <option value="01" selected>Input Text</option>
                    <option value="02">Input Text</option>
                    <option value="03">Input Text</option>
                </select>
            </div>

        </div>

        {# Chips #}
        <h1 class="heading-3xl">Chips</h1>
        <div class="border-y border-blue-100 py-4 lg:py-8">

            <div class="flex flex-col">
                <div class="chip mb-4">Chip Text</div>
                <div class="chip chip--white mb-4">Chip Text</div>
            </div>

        </div>

        {# Filter Chips #}
        <h1 class="heading-3xl">Filter Chips</h1>
        <div class="border-y border-blue-100 py-4 lg:py-8">

            <div class="relative"
                x-data="{ expanded: false, timeout: null }"
                x-on:mouseenter="clearTimeout(timeout)"
                x-on:mouseleave="timeout = setTimeout(() => { expanded = false }, 300)">
                <div :class="{ 'filter-chip open': expanded, 'filter-chip' : !expanded }">
                    <button class="flex items-center" @click="expanded = ! expanded">Filter
